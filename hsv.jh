entity Deelnemer {
	nummer String required
}
entity Onderdeel {
	naam String required,
    omschrijving String,
}

entity Niveau {
	naam String required,
    omschrijving String 
}

entity Persoon {
	naam String required,
    geboorteDatum LocalDate required
}

entity Score {
	punten Double required,
}

entity Team {
	naam String required
}

entity Vereniging {
	naam String required
}

entity Wedstrijd {
	naam String required,
	omschrijving String,
    datum LocalDate required
}


relationship OneToMany {
    Deelnemer to Wedstrijd
    Deelnemer to Persoon
    Deelnemer to Team
    Persoon to Vereniging
	Score to Onderdeel
    Score to Niveau
	Score to Deelnemer
    Team to Vereniging
}

